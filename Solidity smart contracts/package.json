  "main": "hardhat.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "hardhat": "hardhat -v",
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "chain": "npx hardhat node",
    "deploy": "npx hardhat run --network localhost scripts/marketPlaceDeploy.js",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/marketPlaceDeploy.js",
    "deploy:ropsten": "npx hardhat run --network ropsten scripts/marketPlaceDeploy.js",
    "deploy:polygonMumbai": "npx hardhat run --network polygonMumbai scripts/marketPlaceDeploy.js",
    "test": "npx hardhat test",
    "verify:rinkeby": "npx hardhat verify --network rinkeby 0x39c527F51dfc6465FFb7287E5e011FF58D970340",
    "verify:polygonMumbai": "npx hardhat verify --network polygonMumbai 0x4054e158C0D7B11c79176535F2Be96e9255c73C0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@soliditylabs/erc721-permit": "^1.0.0",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "eslint": "8.11.0",
    "ethereum-waffle": "^3.4.4",
    "hardhat": "^2.9.3",
